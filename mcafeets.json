{
    "appid": "d7f2315b-757d-4018-bc4a-be07a4aac294",
    "name": "McAfee TrustedSource",
    "description": "McAfee TrustedSource provides an online service that enables you to check website categorization and risk levels",
    "type": "information",
    "product_vendor": "McAfee",
    "logo": "logo_mcafee.svg",
    "logo_dark": "logo_mcafee_dark.svg",
    "product_name": "TrustedSource",
    "product_version_regex": ".*",
    "publisher": "Martin Ohl",
    "license": "Copyright (c) Martin Ohl, 2019",
    "app_version": "1.0.0",
    "package_name": "phantom_mcafeets",
    "main_module": "mcafeets_connector.pyc",
    "min_phantom_version": "4.0.1068",
    "app_wizard_version": "1.0.0",
    "configuration": {},
    "actions": [
        {
            "action": "test connectivity",
            "identifier": "test_connectivity",
            "description": "Test TrustedSource communication",
            "verbose": "",
            "type": "test",
            "read_only": true,
            "parameters": {},
            "output": [],
            "versions": "EQ(*)"
        },
        {
            "action": "lookup url",
            "identifier": "lookup_url",
            "description": "Check the url categorization and risk level",
            "verbose": "",
            "type": "investigate",
            "read_only": true,
            "parameters": {
                "url": {
                    "description": "URL to lookup",
                    "data_type": "string",
                    "required": true,
                    "primary": true,
                    "contains": [
                        "url",
                        "domain"
                    ],
                    "order": 0
                }
            },
            "output": [
                {
                    "data_path": "action_result.parameter.url",
                    "data_type": "string",
                    "contains": [
                        "url"
                    ],
                    "column_name": "url",
                    "column_order": 0,
                    "example_values": [
                        "google.com"
                    ]
                },
                {
                    "data_path": "action_result.status",
                    "data_type": "string",
                    "column_name": "status",
                    "column_order": 1,
                    "example_values": [
                        "success"
                    ]
                },
                {
                    "data_path": "action_result.message",
                    "data_type": "string",
                    "example_values": [
                        "TrustedSource lookup successful"
                    ]
                },
                {
                    "data_path": "summary.total_objects",
                    "data_type": "numeric",
                    "example_values": [
                        1
                    ]
                },
                {
                    "data_path": "summary.total_objects_successful",
                    "data_type": "numeric",
                    "example_values": [
                        1
                    ]
                },
                {
                    "data_path": "action_result.data.*.status",
                    "data_type": "string",
                    "column_name": "categorized",
                    "column_order": 2,
                    "example_values": [
                        "Categorized URL"
                    ]
                },
                {
                    "data_path": "action_result.data.*.category",
                    "data_type": "string",
                    "column_name": "category",
                    "column_order": 3,
                    "example_values": [
                        "Search Engines"
                    ]
                },
                {
                    "data_path": "action_result.data.*.score",
                    "data_type": "numeric",
                    "example_values": [
                        1
                    ]
                },
                {
                    "data_path": "action_result.data.*.risk",
                    "data_type": "string",
                    "column_name": "risk",
                    "column_order": 4,
                    "example_values": [
                        "Minimal Risk"
                    ]
                }
            ],
            "render": {
                "type": "table"
            },
            "versions": "EQ(*)"
        }
    ]
}